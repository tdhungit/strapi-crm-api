      "required": true,
      "unique": true
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product",
      "mappedBy": "brand"
    },
    "account": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::account.account"
    }
  }
}
