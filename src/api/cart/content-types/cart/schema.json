{
  "kind": "collectionType",
  "collectionName": "carts",
  "info": {
    "singularName": "cart",
    "pluralName": "carts",
    "displayName": "Cart"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "contact": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::contact.contact"
    },
    "subtotal": {
      "type": "decimal",
      "default": 0
    },
    "discount_amount": {
      "type": "decimal",
      "default": 0
    },
    "discount_type": {
      "type": "enumeration",
      "default": "percentage",
      "enum": [
        "percentage",
        "amount"
      ]
    },
    "tax_amount": {
      "type": "decimal",
      "default": 0
    },
    "tax_type": {
      "type": "enumeration",
      "default": "percentage",
      "enum": [
        "percentage",
        "discount"
      ]
    },
    "cart_details": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::cart-detail.cart-detail",
      "mappedBy": "cart"
    }
  }
}
