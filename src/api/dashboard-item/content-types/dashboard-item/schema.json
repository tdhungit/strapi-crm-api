{
  "kind": "collectionType",
  "collectionName": "dashboard_items",
  "info": {
    "singularName": "dashboard-item",
    "pluralName": "dashboard-items",
    "displayName": "Dashboard Item"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "enumeration",
      "enum": ["Widget", "Query"]
    },
    "widget": {
      "type": "string"
    },
    "height": {
      "type": "integer",
      "default": 300
    },
    "weight": {
      "type": "integer",
      "default": 0
    },
    "metadata": {
      "type": "json"
    },
    "dashboard": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::dashboard.dashboard",
      "inversedBy": "dashboard_items"
    }
  }
}
