{
  "kind": "collectionType",
  "collectionName": "payment_methods",
  "info": {
    "singularName": "payment-method",
    "pluralName": "payment-methods",
    "displayName": "Payment Method"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "text"
    },
    "options": {
      "type": "json"
    },
    "enabled": {
      "type": "boolean",
      "default": true
    },
    "payments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::payment.payment",
      "mappedBy": "method"
    }
  }
}
