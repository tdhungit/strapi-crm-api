{
  "kind": "collectionType",
  "collectionName": "states",
  "info": {
    "singularName": "state",
    "pluralName": "states",
    "displayName": "State"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "content-manager": {
      "visible": false
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "code": {
      "type": "string",
      "required": true
    },
    "country": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::country.country",
      "inversedBy": "states"
    },
    "cities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::city.city",
      "mappedBy": "state"
    },
    "latitude": {
      "type": "float"
    },
    "longitude": {
      "type": "float"
    },
    "type": {
      "type": "string"
    },
    "timezone": {
      "type": "string"
    }
  }
}
