{
  "kind": "collectionType",
  "collectionName": "campaigns",
  "info": {
    "singularName": "campaign",
    "pluralName": "campaigns",
    "displayName": "Campaign"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "campaign_type": {
      "type": "enumeration",
      "default": "Email",
      "enum": [
        "Email"
      ]
    },
    "campaign_status": {
      "type": "enumeration",
      "default": "Active",
      "enum": [
        "Active",
        "Inactive"
      ]
    },
    "assigned_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "description": {
      "type": "richtext"
    },
    "leads": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::lead.lead",
      "inversedBy": "campaigns"
    },
    "contacts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::contact.contact",
      "inversedBy": "campaigns"
    },
    "campaign_actions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::campaign-action.campaign-action",
      "mappedBy": "campaign"
    }
  }
}
