{
  "kind": "collectionType",
  "collectionName": "contacts",
  "info": {
    "singularName": "contact",
    "pluralName": "contacts",
    "displayName": "Contact"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "salutation": {
      "type": "enumeration",
      "enum": [
        "Ms.",
        "Mr.",
        "Mrs.",
        "Miss",
        "Dr."
      ]
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "password": {
      "type": "password"
    },
    "login_provider": {
      "type": "enumeration",
      "default": "Local",
      "enum": [
        "Local",
        "Google",
        "Facebook",
        "X"
      ]
    },
    "login_provider_sid": {
      "type": "string"
    },
    "avatar": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "mobile": {
      "type": "string"
    },
    "bod": {
      "type": "date"
    },
    "jobTitle": {
      "type": "string"
    },
    "department": {
      "type": "string"
    },
    "leadSource": {
      "type": "enumeration",
      "enum": [
        "Campaign",
        "Website"
      ]
    },
    "address": {
      "type": "component",
      "component": "common.address",
      "repeatable": false
    },
    "description": {
      "type": "richtext"
    },
    "assigned_user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "contacts"
    },
    "account": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::account.account",
      "inversedBy": "contacts"
    },
    "campaigns": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::campaign.campaign",
      "mappedBy": "contacts"
    },
    "sale_orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sale-order.sale-order",
      "mappedBy": "contact"
    },
    "lead": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::lead.lead",
      "inversedBy": "contact"
    },
    "contact_addresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::contact-address.contact-address",
      "mappedBy": "contact"
    }
  }
}
