{
  "kind": "collectionType",
  "collectionName": "product_variants",
  "info": {
    "singularName": "product-variant",
    "pluralName": "product-variants",
    "displayName": "Product Variant"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "sku": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "photos": {
      "type": "json"
    },
    "variant_status": {
      "type": "enumeration",
      "required": true,
      "default": "Active",
      "enum": [
        "Active",
        "Inactive"
      ]
    },
    "weight": {
      "type": "float"
    },
    "weight_unit": {
      "type": "string"
    },
    "taxable": {
      "type": "boolean",
      "default": true
    },
    "barcode": {
      "type": "string"
    },
    "requires_shipping": {
      "type": "boolean",
      "default": true
    },
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "product_variants"
    },
    "product_variant_attributes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-variant-attribute.product-variant-attribute",
      "mappedBy": "product_variant"
    },
    "product_prices": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-price.product-price",
      "mappedBy": "product_variant"
    },
    "inventories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::inventory.inventory",
      "mappedBy": "product_variant"
    },
    "purchase_order_details": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::purchase-order-detail.purchase-order-detail",
      "mappedBy": "product_variant"
    },
    "sale_order_details": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sale-order-detail.sale-order-detail",
      "mappedBy": "product_variant"
    }
  }
}
